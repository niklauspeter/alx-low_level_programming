#include "main.h"

/**
 * _strcmp - function compares two strings
 * @s1: input takes in string 
 * @s2: input takes in string
 * Retun: difference s1 and s2:
*/

int _strcmp (char *s1, char *s2)
{
	while (*s1 && *s2)
	{
		if (*s1 != *s2)
			return (*s1 - *s2);
		s1++, s2++;
	}
	return (*s1 - *s2);
}
